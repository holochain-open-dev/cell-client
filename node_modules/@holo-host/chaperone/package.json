{
  "name": "@holo-host/chaperone",
  "version": "7.1.2",
  "description": "Holo's restricted web zone for managing an Agent's identity and connections",
  "main": "build/index.js",
  "module": "build/index.js",
  "browser": "dist/holo_hosting_chaperone.js",
  "bin": {
    "chaperone-server": "bin/chaperone-server"
  },
  "scripts": {
    "prepare": "tsc --esModuleInterop --lib es2015,dom --outDir ./build -p ./src && npx webpack --mode production --output-filename holo_hosting_chaperone.js && cp public/* dist/",
    "build": "npm run compile && npm run bundle",
    "compile": "tsc --esModuleInterop --lib es2015,dom --outDir ./build -p ./src",
    "bundle": "npx webpack --mode production --output-filename holo_hosting_chaperone.js && cp public/* dist/",
    "bundle-dev": "npx webpack --mode development --output-filename holo_hosting_chaperone.js && cp public/* dist/",
    "test-unit": "mocha ./tests/unit/"
  },
  "author": "Holo Ltd.",
  "contributors": [
    "Lisa Jetton<lisa.jetton@holo.host>",
    "Joel U <joel@holo.host>",
    "Robbie Carlton <robbie.carlton@holo.host>",
    "Matthew Brisebois <matthew.brisebois@holo.host>"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Holo-Host/chaperone/"
  },
  "files": [
    "README.md",
    "dist/*",
    "bin/*",
    "build/*",
    "rpc-websocket/*"
  ],
  "dependencies": {
    "@holo-host/comb": "^0.2.0",
    "@holo-host/cryptolib": "^0.3.0",
    "@holo-host/data-translator": "^0.1.1",
    "@holo-host/mock-conductor": "^0.3.2",
    "@whi/http_utils": "^0.1.0",
    "@whi/printf": "^1.0.0",
    "@whi/stdlog": "^0.3.1",
    "commander": "^5.0.0",
    "json-stable-stringify": "^1.0.1",
    "rpc-websockets": "^4.6.1",
    "sprintf-js": "^1.1.2",
    "tiny-msgpack": "^1.0.2"
  },
  "devDependencies": {
    "@holochain/conductor-api": "^0.0.4",
    "@types/node": "^12.11.1",
    "braintree-jsdoc-template": "^3.3.0",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "fetch-mock": "^8.3.2",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.1",
    "node-fetch": "^2.6.0",
    "puppeteer": "^2.1.1",
    "typescript": "^3.6.4",
    "waait": "^1.0.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
